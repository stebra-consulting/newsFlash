@model List<ExternalWebsite2.Models.StebraEntity>

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Archived News</h1>
    <p class="lead">Introducing our nostalgic archived news</p>
</div>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-12">
            <h2>@item.Title</h2>
            <p><i>@item.Date @Html.Raw(item.Article)</i></p>
            <p>
                @Html.Raw(item.Body)
            </p>
        </div>
    }
</div>



<script>
    $("#thisyear").click(function () {
        $("#thisyearold").toggle();
    });

    $("#oneyearbehind").click(function () {
        $("#oneyearold").toggle();
    });

    $("#twoyearsbehind").click(function () {
        $("#twoyearsold").toggle();
    });
</script>